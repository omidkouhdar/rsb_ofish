<p>
    <a class="btn btn-success" asp-controller="Ofish" asp-action="Create">ثبت مورد جدید <i class="icon-plus"></i></a>
</p>


<div id="dataList">

</div>




@section Scripts{
    <script src="~/lib/Sweetalert2/sweetalert2.min.js"></script>
    <link rel="stylesheet" href="~/lib/Sweetalert2/sweetalert2.min.css" />
    <script>

        function getData(pageid) {
            var url = '/home/getTodayData';
            data = { pageId: pageid };
            $.ajax({
                url: url,
                data: data,
                type: 'get',
                success: function (result) {
                    $('#dataList').html(result);
                },
                error: function (err) {

                }

            });
        }
    </script>
    <script>
        $(function () {
            getData(1);
        });
    </script>
    @{ await Html.RenderPartialAsync("_oprationJsView");}

}